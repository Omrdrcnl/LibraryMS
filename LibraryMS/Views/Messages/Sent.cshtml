@using LibraryMS.Models.Entity
@model List<tblMessages>

@{
    ViewBag.Title = "Sent";
    Layout = "~/Views/Shared/LayoutPanel.cshtml";
}

<h2>Giden Mesajlar Sayfası</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Alıcı</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Tarih</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Acceptor</td>
            <td>@x.Subject</td>
            <td>@x.Details </td>
            <td>@x.Date</td>
        </tr>
    }
</table>